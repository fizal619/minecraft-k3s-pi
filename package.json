{
  "name": "minecraft",
  "version": "1.0.0",
  "description": "These configuration files were meant for a pi server running on a Raspberry Pi 4 4GB.",
  "main": "index.js",
  "scripts": {
    "test": "exit 0",
    "start-paper": "java -Xms1G -Xmx1G -XX:+UseG1GC -XX:+UnlockExperimentalVMOptions -XX:MaxGCPauseMillis=100 -XX:+DisableExplicitGC -XX:TargetSurvivorRatio=90 -XX:G1NewSizePercent=50 -XX:G1MaxNewSizePercent=80 -XX:G1MixedGCLiveThresholdPercent=35 -XX:+AlwaysPreTouch -XX:+ParallelRefProcEnabled -Dusing.aikars.flags=mcflags.emc.gs -jar paperclip.jar",
    "start-dev": "curl --output paperclip.jar -L https://papermc.io/api/v1/paper/1.15.1/25/download && npm run start-paper",
    "build-sellout": "cd sellout_src && mvn clean compile assembly:single && cd .. && cp sellout_src/target/SelloutPlugin-* plugins/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fizal619/minecraft-server-pi.git"
  },
  "keywords": [],
